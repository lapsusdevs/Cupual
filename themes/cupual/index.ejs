<body class="bg-white">
<div class="relative">
    <!-- Navbar -->
    <header x-data="{ isOpen: false }" class="fixed top-0 w-full z-50">
        <!-- Backdrop blur container -->
        <div class="absolute inset-0 z-50 bg-gray-200/20 rounded-3xl backdrop-blur border border-black/5 shadow-sm"></div>
        
        <!-- Navbar content -->
        <nav class="relative mx-auto max-w-7xl px-6 lg:px-8 py-4 z-50">
            <div class="flex items-center justify-between">
                <!-- Logo -->
                <div class="flex lg:flex-1">
                    <a href="#" class="flex items-center space-x-2">
                        <img src="<%= settings.logo.url %>" class="h-10">
                    </a>
                </div>

                <!-- Mobile menu button -->
                <div class="flex lg:hidden">
                    <button @click="isOpen = !isOpen" type="button" class="inline-flex items-center justify-center rounded-md p-2.5 text-gray-700">
                        <span class="sr-only">Open main menu</span>
                        <svg class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" aria-hidden="true">
                            <path stroke-linecap="round" stroke-linejoin="round" d="M3.75 6.75h16.5M3.75 12h16.5m-16.5 5.25h16.5"></path>
                        </svg>
                    </button>
                </div>

                <!-- Desktop nav links -->
                <div class="hidden lg:flex lg:gap-x-12 z-50">
                    <a href="https://status.<%= settings.main_domain %>/" class="text-sm font-semibold leading-6 text-gray-900 hover:text-indigo-900 transition-colors">Status</a>
                    <a href="<%= settings.discord_server %>" class="text-sm font-semibold leading-6 text-gray-900 hover:text-indigo-900 transition-colors">Discord Server</a>
                    <a href="/auth" class="text-sm font-semibold leading-6 text-gray-900 hover:text-indigo-900 transition-colors">Dashboard</a>
                </div>

                <!-- Desktop CTA buttons -->
                <div class="hidden lg:flex lg:flex-1 lg:justify-end lg:gap-x-6">

                    <a href="/auth" class="rounded-lg bg-gray-950 px-4 py-2 text-sm font-semibold text-white shadow-sm hover:bg-gray-800 hover:text-indigo-300 transition-colors">Get started</a>

                </div>
            </div>

            <!-- Mobile menu -->
            <div x-show="isOpen" class="lg:hidden" role="dialog" aria-modal="true" style="display: none;">
                <div class="fixed inset-0 z-50"></div>
                <div class="fixed inset-y-0 right-0 z-50 w-full overflow-y-auto bg-white px-6 py-6 sm:max-w-sm sm:ring-1 sm:ring-gray-900/10">
                    <!-- Mobile menu header -->
                    <div class="flex items-center justify-between">
                        <a href="#" class="flex items-center space-x-2">
                            <span class="text-xl font-semibold text-gray-900"><%= settings.name %></span>
                        </a>
                        <button @click="isOpen = false" type="button" class="rounded-md p-2.5 text-gray-700">
                            <span class="sr-only">Close menu</span>
                            <svg class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" aria-hidden="true">
                                <path stroke-linecap="round" stroke-linejoin="round" d="M6 18L18 6M6 6l12 12"></path>
                            </svg>
                        </button>
                    </div>
                    <!-- Mobile menu links -->
                    <div class="mt-6 flow-root">
                        <div class="space-y-2 py-6">
                    <a href="https://status.<%= settings.main_domain %>" class="text-sm font-semibold leading-6 text-gray-900 hover:text-gray-600 transition-colors">Status</a>
                    <a href="<%= settings.discord_server %>" class="text-sm font-semibold leading-6 text-gray-900 hover:text-gray-600 transition-colors">Discord Server</a>
                    <a href="/auth" class="text-sm font-semibold leading-6 text-gray-900 hover:text-gray-600 transition-colors">Dashboard</a>
                        </div>
                        <div class="border-t border-gray-200 py-6">
                            <a href="/auth" class="mt-4 block rounded-lg bg-gray-950 px-3 py-2.5 text-center text-base font-semibold text-white shadow-sm hover:bg-[#0b0e24] transition-colors">Get started</a>
                        </div>
                    </div>
                </div>
            </div>
        </nav>
    </header>

  <div class="mx-auto max-w-7xl lg:grid lg:grid-cols-12 lg:gap-x-24 lg:px-8">
    <div class="px-8 pb-24 ml-24 pt-12 sm:pb-32 text-center lg:col-span-7 lg:col-start-3 z-40 lg:px-0 lg:py-64 flex flex-col justify-end items-center">
      <!-- Added Badge -->
      <div class="mb-8 inline-flex items-center rounded-full px-4 py-1 backdrop-blur bg-white/30 shadow-sm ring-1 ring-gray-900/10">
        <svg class="mr-2 h-4 w-4 text-indigo-600" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
          <path d="M12 3l1.912 5.813a2 2 0 001.225 1.225L21 12l-5.813 1.912a2 2 0 00-1.225 1.225L12 21l-1.912-5.813a2 2 0 00-1.225-1.225L3 12l5.813-1.912a2 2 0 001.225-1.225L12 3z"></path>
        </svg>
        <span class="text-sm font-medium text-gray-900">Now available</span>
      </div>
      
      <h2 class="text-3xl leading-tight tracking-tight sm:text-4xl md:text-5xl lg:text-6xl font-semibold text-gray-900 md:text-balance">
        Your favourite dashboard, now with a new look.
      </h2>
      <!-- Added Text Below -->
      <p class="mt-6 text-lg leading-8 text-gray-600 max-w-2xl mx-auto">
        From game servers to websites, deploy server easier than ever.
        The new hosting software platform is here - ready for you.
