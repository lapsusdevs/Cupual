<html><head>
    <!-- Character Set -->
    <meta charset="UTF-8">
    
    <!-- Alpine -->
    <script defer="" src="https://cdn.jsdelivr.net/npm/alpinejs@3.x.x/dist/cdn.min.js"></script>

    <!-- Viewport -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=5.0">
    
    <!-- Primary Meta Tags -->
    <title><%= settings.name %></title>
    <meta name="title" content="<%= settings.name %> - The Best Cloud Hosting Platform">
    <meta name="description" content="Launch your games, web apps, and more with enterprise-grade infrastructure - completely free forever. Deploy instantly across 6 global regions with zero hidden costs.">
    <meta name="keywords" content="free hosting, game server hosting, web hosting, cloud platform, free cloud hosting, zero cost hosting">
    <meta name="author" content="Deploy Now">
    
    <!-- Favicon -->
    <link rel="apple-touch-icon" sizes="180x180" href="<%= settings.logo.url %>">
    <link rel="icon" type="image/png" sizes="32x32" href="<%= settings.logo.url %>">
    <link rel="icon" type="image/png" sizes="16x16" href="<%= settings.logo.url %>">
    <link rel="manifest" href="/site.webmanifest">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#4f46e5">
    <meta name="msapplication-TileColor" content="#4f46e5">
    <meta name="theme-color" content="#ffffff">
    
    <!-- Resources -->
    <script src="https://cdn.tailwindcss.com"></script>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@100..900&amp;display=swap" rel="stylesheet">
    
    <!-- Additional Meta Tags -->
    <meta name="robots" content="index, follow">
    <meta name="language" content="English">
    <meta name="revisit-after" content="7 days">
    
    <!-- DNS Prefetch -->
    <link rel="dns-prefetch" href="//fonts.googleapis.com">
    <link rel="dns-prefetch" href="//cdn.tailwindcss.com">
    
    <!-- Preconnect -->
    <link rel="preconnect" href="https://fonts.googleapis.com" crossorigin="">
    <link rel="preconnect" href="https://cdn.tailwindcss.com" crossorigin="">
    
    <style>
        body {
            font-family: 'Inter', sans-serif;
        }
        .gradient-blur {
            background: radial-gradient(circle at top right, rgba(79, 70, 229, 0.08) 0%, transparent 60%);
        }
        @keyframes float {
            0% { transform: translateY(0px); }
            50% { transform: translateY(-10px); }
            100% { transform: translateY(0px); }
        }
        .float-animation {
            animation: float 6s ease-in-out infinite;
        }
    </style>
    
    <!-- JSON-LD Schema -->
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "SoftwareApplication",
        "name": "Cupual",
        "url": "",
        "logo": "<%= settings.logo.url %>",
        "description": "The best cloud hosting platform with enterprise-grade infrastructure",
        "applicationCategory": "Cloud Platform",
        "operatingSystem": "Any",
        "offers": {
            "@type": "Offer",
            "price": "0",
            "priceCurrency": "EUR"
        },
        "features": [
            "Game server hosting",
            "Web application hosting",
            "6 global regions",
            "Enterprise-grade infrastructure",
            "Zero hidden costs",
            "Instant deployment"
        ],
        "contactPoint": {
            "@type": "ContactPoint",
            "contactType": "customer support",
            "url": ""
        }
    }
    </script>
