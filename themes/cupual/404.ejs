<body class="bg-[#010103] min-h-screen flex items-center justify-center p-4">
    <div class="max-w-2xl w-full">
        <!-- Main Error Card -->
        <div class="relative overflow-hidden bg-white/[0.02] backdrop-blur-xl border border-white/10 rounded-3xl p-8 md:p-12">
            <!-- Decorative gradient background -->
            <div class="absolute inset-0 bg-gradient-to-b from-indigo-500/10 to-transparent pointer-events-none"></div>
            
            <!-- Content -->
            <div class="relative z-10 text-center">
                <!-- Status Badge -->
                <div class="inline-flex items-center px-3 py-1 rounded-full text-xs font-medium bg-indigo-500/20 text-indigo-300 mb-8">
                    <span class="size-2 rounded-full bg-indigo-400 mr-2"></span>
                    Not found
                </div>

                <!-- Title -->
                <h1 class="text-4xl font-semibold text-white mb-4 tracking-tight">
                    We ran into a problem
                </h1>

                <!-- Description -->
                <p class="text-lg text-white/60 mb-8 max-w-xl mx-auto">
                    We're having trouble connecting to our servers. This is usually temporary and will be resolved shortly.
                </p>

                <!-- Actions -->
                <div class="space-y-4">
                    <!-- Primary Button -->
                    <a href="/" class="inline-flex items-center justify-center h-11 gap-2 px-6 text-sm font-medium text-white transition-all duration-200 rounded-full bg-indigo-500 hover:bg-indigo-600 focus:ring-2 focus:ring-indigo-500 focus:ring-offset-2 ring-offset-[#010103]">
                        Try again
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" class="w-4 h-4">
                            <path stroke-linecap="round" stroke-linejoin="round" d="M13.5 4.5L21 12m0 0l-7.5 7.5M21 12H3"></path>
                        </svg>
                    </a>

                    <!-- Secondary Button -->
                    <a href="<%= settings.discord_server %>" class="block text-sm text-white/50 hover:text-white/70 transition-colors">
                        Need help? Join our Discord
                    </a>
                </div>
            </div>

            <!-- Decorative circles -->
            <div class="absolute -top-24 -right-24 w-96 h-96 bg-indigo-500/10 rounded-full blur-3xl pointer-events-none"></div>
            <div class="absolute -bottom-24 -left-24 w-96 h-96 bg-indigo-500/10 rounded-full blur-3xl pointer-events-none"></div>
        </div>

        <!-- Server Status -->
        <div class="mt-6 text-center">
            <a href="https://status.<%= settings.main_domain %>" class="inline-flex items-center gap-2 px-4 py-2 text-sm text-white/70 bg-white/5 hover:bg-white/10 transition rounded-full">
                <span class="relative flex h-2 w-2">
                    <span class="animate-ping absolute inline-flex h-full w-full rounded-full bg-indigo-400 opacity-75"></span>
                    <span class="relative inline-flex rounded-full h-2 w-2 bg-indigo-500"></span>
                </span>
                Check our status page
            </a>
        </div>
    </div>
